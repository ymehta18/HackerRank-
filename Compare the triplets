vector<int> compareTriplets(vector<int> a, vector<int> b) {
    vector<int> comp;
    int x,y;
    x=y=0;
    for (int i=0;i<a.size();i++) {
        if (a[i] > b[i]) 
            x++;
        else if (a[i] < b[i]) 
            y++;
    }
    comp.push_back(x);
    comp.push_back(y);
    return comp;
}
